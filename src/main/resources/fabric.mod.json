{
  "schemaVersion": 1,
  "id": "autodisconnect",
  "version": "${version}",

  "name": "AutoDisconnect",
  "description": "Automagically disconnects when a player comes into render distance",

  "authors": [
    "SpaceClouds42"
  ],

  "contact": {
    "email": "SpaceClouds42@gmail.com",
    "homepage": "https://github.com/SpaceClouds42/AutoDisconnect",
    "issues": "https://github.com/SpaceClouds42/AutoDisconnect/issues",
    "sources": "https://github.com/SpaceClouds42/AutoDisconnect"
  },

  "license": "LGPL-3.0",

  "environment": "*",

  "entrypoints": {
    "main": [
      {
        "value": "autodisconnect.Common::INSTANCE"
      }
    ]
  },

  "mixins": [
    "autodisconnect.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.8.7",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": ">=1.16"
  },

  "suggests": {

  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}
